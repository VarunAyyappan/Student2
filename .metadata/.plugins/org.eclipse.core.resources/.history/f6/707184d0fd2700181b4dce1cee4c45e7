package jrJava.hashCodes;


public class Test {

	public static void main(String[] args) {
		//Object ref1 = new Thing(1, 1, 1);
		//System.out.println(ref1.getClass());

		Thing ref1 = new Thing(1, 2, 3);
		Thing ref2 = new Thing(1, 2, 3);
		System.out.println(ref1.equals(ref2));

		System.out.println(ref1.toString());

		int hashCode = ref1.hashCode();
		System.out.println(hashCode);
	}

}
