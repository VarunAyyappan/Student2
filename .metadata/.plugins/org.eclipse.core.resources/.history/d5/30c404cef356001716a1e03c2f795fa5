package jrJava.recursion2;

public class Fibonacci {

	public static void main(String[] args) {
		System.out.println(fibonacciRecursion(3));
	}
	
	public static int fibonacciRecursion(int n) {
		if (n==1 || n==2) return 1;
		
		int first = 1;
		int second = 1;
		int third = 0;
		
		for(int i=3; i<=n; i--) {
			third = second + first;
			first = second;
			second = third;
		}
		
		return third;
	}
	
	public static int fibonacci(int n) {
		for(int i =1; i<=n; i++) {
			
		}
	}
	
}
